    "A, B, V = map(int, input().split())\n",
    "quotient  = (V - A) // (A - B)\n",
    "remainder  = (V - A) % (A - B)\n",
    "answer = quotient + 1 + (1 if remainder > 0 else 0)\n",
    "print(answer)\n"
   ]
